{
  "private": true,
  "name": "Vanderfool",
  "description": "Preact's netlify CMS template",
  "version": "0.1.3",
  "license": "MIT",
  "scripts": {
	"start": "per-env",
	"start:production": "npm run -s serve",
	"start:development": "npm run -s dev",
	"build": "preact build --prerenderUrls ./prerender-urls.js",
	"dev": "preact watch --prerenderUrls ./prerender-urls.js",
	"lint": "eslint src",
	"test": "jest"
  },
  "eslintConfig": {
	"extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
	"build/*"
  ],
  "devDependencies": {
	"eslint": "^4.9.0",
	"eslint-config-synacor": "^2.0.2",
	"esm": "^3.2.25",
	"glob": "^7.1.4",
	"jest": "^21.2.1",
	"jest-preset-preact": "^1.0.0",
	"markdown": "^0.5.0",
	"node-sass": "^4.14.1",
	"per-env": "^1.0.2",
	"preact-cli": "^3.0.1",
	"preact-render-spy": "^1.2.1",
	"react-syntax-highlighter": "^12.2.1",
	"sass-loader": "^9.0.3",
	"webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
	"@preact/prerender-data-provider": "^3.0.1",
	"imagemin-mozjpeg": "^8.0.0",
	"imagemin-webpack-plugin": "^2.4.2",
	"markdown-to-jsx": "^6.11.4",
	"parse-md": "^2.0.4",
	"preact": "^10.4.6",
	"preact-cli-plugin-netlify": "^1.6.0",
	"preact-render-to-string": "^5.1.10",
	"preact-router": "^3.0.1"
  },
  "jest": {
	"preset": "jest-preset-preact"
  },
  "bugs": {
	"url": "https://github.com/jackokaiser/vanlab/issues"
  },
  "homepage": "https://github.com/jackokaiser/vanlab#readme",
  "keywords": [
	"preact",
	"netlify"
  ],
  "repository": {
	"type": "git",
	"url": "git+https://github.com/jackokaiser/vanlab.git"
  }
}
